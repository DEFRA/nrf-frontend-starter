{% extends 'layouts/page.njk' %}

{% block content %}
  <h1 class="govuk-heading-xl">{{ heading }}</h1>

  <p class="govuk-body">
    This prototype contains different user journeys that will be trialled with stakeholders. Select a journey below to access the journey.
  </p>

  <table class="govuk-table">
    <thead class="govuk-table__head">
      <tr class="govuk-table__row">
        <th scope="col" class="govuk-table__header">User Journey</th>
        <th scope="col" class="govuk-table__header">Description</th>
      </tr>
    </thead>
    <tbody class="govuk-table__body">
      <tr class="govuk-table__row">
        <td class="govuk-table__cell">
          <a href="/contact-form" class="govuk-link">Contact form</a>
        </td>
        <td class="govuk-table__cell">
          Submit enquiries and get responses about the Nature Restoration Fund.
        </td>
      </tr>
    </tbody>
  </table>

{% endblock %}